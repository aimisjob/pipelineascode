pipeline{
 agent any
  stages{
   stage('build master'){
      when{
       branch 'master'
      }
      steps{
       echo 'building master'
       }
       }
     stage('build devv'){
      when{
        branch 'devv'
       }
       steps{
         echo 'building dev'
       }
      }
     }
     }
     
     
       
       
         
         
         
         
        
     
       
     
